/*1 列表封装*/
.cartList-item{
  display: flex;
  padding: 20rpx;
  font-size: 16px;
  color: #666;
  border-bottom: 2rpx solid #eee;
}
.cartList-item image{
  width: 160rpx;
  height: 160rpx;
}
/*2 封隔器*/
.devision{
  width: 100%;
  height: 30rpx;
  background-color: #f5f5f5;
}
/*3 箭头封装 */
.arrow{
  display: inline-block;
  position: absolute;
  top: 35rpx;
  right: 40rpx;
  width: 16rpx;
  height: 16rpx;
  border-top: 4rpx solid #ddd;
  border-right:4rpx solid #ddd;
  transform: rotate(45deg);
}

