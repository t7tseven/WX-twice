.addAddress{
  height: 90rpx;
  width: 100%;
  line-height: 90rpx;
  text-align: center;
  color: #ddd;
  font-size: 14px;
  position: relative;
}
.user{
  width: 100%;
  padding: 0 20rpx;
  height: 150rpx;
  line-height: 50rpx;
  display: flex;
  flex-direction: column;
  color: #444;
  font-size: 14px;
}
.cartList-details{
  margin-left: 20rpx;
  font-size: 14px;
  line-height: 50rpx;
  display: flex;
  flex-direction: column;
}
.pay{
  width: 100%;
  height: 100rpx;
  border-top: 2rpx solid #ddd;
  position: absolute;
  bottom: 0;
  color: #ab956d;
  font-size: 14px;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 20rpx;
}
.gopay{
  width: 240rpx;
  height: 100rpx;
  background-color: #ab956d;
  color: #fff;
  line-height: 100rpx;
  text-align: center;
}